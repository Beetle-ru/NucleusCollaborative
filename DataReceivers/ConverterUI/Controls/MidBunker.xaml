        <Label Canvas.Left="112" Canvas.Top="51" FontSize="12" FontWeight="Bold" Height="16" HorizontalContentAlignment="Left" Name="lMaterial1Weight" Padding="0" VerticalContentAlignment="Center" Width="42" />
        <Label Canvas.Left="112" Canvas.Top="71" FontSize="12" FontWeight="Bold" Height="16" HorizontalContentAlignment="Left" Name="lMaterial2Weight" Padding="0" VerticalContentAlignment="Center" Width="42" />
        <Label Canvas.Left="112" Canvas.Top="91" FontSize="12" FontWeight="Bold" Height="16" HorizontalContentAlignment="Left" Name="lMaterial3Weight" Padding="0" VerticalContentAlignment="Center" Width="42" />
        <Label Canvas.Left="112" Canvas.Top="111" FontSize="12" FontWeight="Bold" Height="16" HorizontalContentAlignment="Left" Name="lMaterial4Weight" Padding="0" VerticalContentAlignment="Center" Width="42" />
