        <Label Canvas.Left="51" Canvas.Top="71" Content="ПБ" FontSize="12" FontWeight="Normal" Height="16" HorizontalContentAlignment="Right" Name="lMaterial2Name" Padding="0" VerticalContentAlignment="Center" Width="60" />
